# Replace with your n8n endpoint (not Ollama)
OLLAMA_BASE_URL="YOUR_N8N_API_ENDPOINT_URL"

# Security Essentials
WEBUI_AUTH=true
WEBUI_SECRET_KEY=your_generated_key_here  # Run: openssl rand -base64 32

# Privacy
DO_NOT_TRACK=true
SCARF_NO_ANALYTICS=true
ANONYMIZED_TELEMETRY=false

# Pipeline Config (add these new lines)
DEFAULT_PIPELINE=client-chat
OPENWEBUI_PIPELINES_CONFIG=./config/pipelines.yaml
